plot \
'results.txt' using 1:2:(0.01) smooth acsplines lt rgb "red" title 'Wait time on intersection(for all cars)', \
'results.txt' using 1:2:(0.000001) smooth acsplines lw 3 lt rgb "red" title 'Wait time on intersection(for all cars)', \
'results.txt' using 1:3 lt rgb "red" title 'Max wait time', \
'results_with_precognition.txt' using 1:2:(0.001) smooth acsplines lt rgb "green" title 'Wait time on intersection(for all cars) with Precognition', \
'results_with_precognition.txt' using 1:2:(0.000001) smooth acsplines lw 3 lt rgb "green" title 'Wait time on intersection(for all cars) with Precognition', \
'results_with_precognition.txt' using 1:3 lt rgb "green" title 'Max wait time with Precognition'
